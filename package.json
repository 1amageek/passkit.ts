    "node-forge": "^0.8.5",
    "request": "^2.88.2",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/archiver": "^3.1.1",
    "@types/jest": "^26.0.14",
    "@types/mkdirp": "^1.0.1",
